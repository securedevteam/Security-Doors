@model CardViewModel

<h2>Информация карточки №@Model.Id</h2>
<table>
    <thead>
        <tr>
            <th>Уникальный номер</th>
            <th>Уровень доступа</th>
            <th>Нахождение в данный момент (Вход - в здании / Выход - за пределами):</th>
            <th>Статус</th>
            <td>Комментарий</td>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.UniqueNumber</td>
            <td>@Model.Level</td>
            <td>@Model.Location</td>
            <td>@Model.Status</td>
            <td>@Model.Comment</td>
        </tr>
    </tbody>
</table>
